<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dancun">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" /> <!-- 精确定位权限，允许一个程序访问精确位置(GPS定位) -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icons"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icons"
        android:supportsRtl="true"
        android:theme="@style/Theme.leave"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.SalesLeaveActivity"></activity>
        <activity android:name=".activity.MapActivity" /> <!-- <activity android:name=".activity.SplashActivity"/> -->
        <activity android:name=".activity.LeaveStateActivity" />
        <activity android:name=".activity.LeaveActivity" />
        <activity android:name=".activity.LeaveDetail" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>